{
	"compilerOptions": {
		"target": "ES2020",
		"module": "CommonJS",
		"lib": [
			"ESNext",
			"DOM"
		],
		"outDir": "./dist",
		"removeComments": true,
		"strict": true,
		"noEmitOnError": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true
	}
}